{
	"name": "imagesmasher",
	"version": "7.7.7",
	"description": "Minify images",
	"type": "module",
	"bin": {
		"imagesmasher": "./cli.js"
	},
	"scripts": {
		"test": "node cli.js \"test.png\""
	},
	"eslintConfig": {
		"root": true,
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"extends": [
			"eslint:recommended",
			"prettier"
		],
		"env": {
			"node": true,
			"es6": true
		}
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"semi": false,
		"singleQuote": true
	},
	"devDependencies": {
		"eslint": "^8.16.0",
		"prettier": "^2.7.1"
	},
	"dependencies": {
		"chalk": "^5.0.1",
		"file-type": "^18.0.0",
		"globby": "^13.1.2",
		"imagemin-gifsicle": "^7.0.0",
		"imagemin-jpegtran": "^7.0.0",
		"imagemin-mozjpeg": "^10.0.0",
		"imagemin-pngquant": "^9.0.2",
		"imagemin-svgo": "^10.0.1",
		"ora": "^6.1.2",
		"pino": "^8.5.0",
		"pino-pretty": "^9.1.0",
		"plur": "^5.1.0",
		"promise.pipe": "^3.0.0",
		"svgo-autocrop": "^1.1.2"
	}
}
